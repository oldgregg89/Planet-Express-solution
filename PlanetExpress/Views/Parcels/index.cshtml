@using PlanetExpress.Models;

<h1>PLanet Express Shipping</h1>

@if (Model.Count == 0)
{
  <p>There are no Parcels in the list.</p>
}
<ul>
  <h4>Good news everyone we have parcels!</h4>
  @foreach (Parcel item in Model)
  {
    <li> inchs @item.Height, inchs @item.Width, inchs @item.Length, Lb @item.Weight, $@item.GetPrice, @item.Description, Vol. @item.GetVolume</li>
  }

</ul>
<a href="/Cars/new">Add a new Parcel.</a>